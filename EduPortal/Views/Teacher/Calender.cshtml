@{
     Layout = "_LayoutAdmin";
}
 <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.22.2/moment.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/fullcalendar/3.8.0/fullcalendar.min.js"></script>


<div class="loader"></div>

        <section class="section">
          <div class="section-body">
            <div class="row">
              <div class="col-12">
                <div class="card">
                  <div class="card-header">
                    <h4>Calendar</h4>
                  </div>
                  <div class="card-body">
                    <div class="fc-overflow">
                      <div id="myEvent"></div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </section>   
<script type="text/javascript">
  
        var tasks = @Html.Raw(Newtonsoft.Json.JsonConvert.SerializeObject(ViewBag.ToDo)); 
        var myList=new Array;
        for (var i = 0;i < tasks.length;i++){
            
        let TaskCard = {}; 
        TaskCard.title =tasks[i].TaskTitle;
        //TaskCard.start = movies[i].StartAt;
        //TaskCard.end = movies[i].StartAt;
        TaskCard.start =tasks[i].EndDate;
        TaskCard.end = tasks[i].EndDate;
        TaskCard.backgroundColor = "#dc3545";
        myList.push(TaskCard);
        }


var calendar = $('#myEvent').fullCalendar({
  height: 'auto',
  defaultView: 'month',
  editable: false,
  selectable: false,
  header: {
    left: 'prev,next',
    center: 'title',
    right: 'month,listMonth'
  },
  events: myList
});
</script>